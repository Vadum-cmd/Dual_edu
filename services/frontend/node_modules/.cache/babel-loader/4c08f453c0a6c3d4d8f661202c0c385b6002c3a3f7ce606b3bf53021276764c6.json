ogress {\r\n  height: 100%;\r\n  background-color: #666;\r\n  color: white;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  font-weight: bold;\r\n}\r\n\r\n.profile-lines {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  text-align: center;\r\n  width: 100%;\r\n}\r\n\r\n.profile-line {\r\n  width: 100%;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.profile-line h3 {\r\n  margin: 0;\r\n  font-family: Arial, sans-serif;\r\n}\r\n\r\n.profile-line p {\r\n  margin: 0;\r\n  font-family: Arial, sans-serif;\r\n  color: #666;\r\n  line-height: 1.5;\r\n}</style>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}